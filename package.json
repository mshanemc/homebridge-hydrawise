  },
  "devDependencies": {
    "@types/node": "10.17.19",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "eslint": "^7.4.0",
    "eslint-plugin-prettier": "^3.1.4",
    "homebridge": ">=1.0.0",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.8.3"
